<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>InfoS-web</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>SupportServlet</servlet-name>
    <servlet-class>iac.grn.infosweb.servlet.support.SupportServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SupportServlet</servlet-name>
    <url-pattern>/servlet/support</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FileUploadServlet</servlet-name>
    <servlet-class>iac.grn.infosweb.servlet.support.FileUploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUploadServlet</servlet-name>
    <url-pattern>/servlet/fileupload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>*.seam</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>StartupServlet</display-name>
    <servlet-name>StartupServlet</servlet-name>
    <servlet-class>ru.spb.iac.cud.infosweb.web.init.StartupServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>StartupServlet</servlet-name>
    <url-pattern>/StartupServlet</url-pattern>
  </servlet-mapping>
  
  <context-param>
    <param-name>SIGN_REQUIRED</param-name>
    <param-value>true</param-value>
  </context-param>
  <context-param>
    <param-name>SAML_REQUEST_LOGIN</param-name>
    <param-value>/home/jboss/jboss/data/saml/saml_request_post.xml</param-value>
  </context-param>
  <context-param>
    <param-name>SAML_REQUEST_LOGOUT</param-name>
    <param-value>/home/jboss/jboss/data/saml/saml_request_logout_post.xml</param-value>
  </context-param>
  <context-param>
    <param-name>SAML_ASSERTION</param-name>
    <param-value>/home/jboss/jboss/data/saml/saml_asserion.xml</param-value>
  </context-param>
  <context-param>
    <param-name>STORE_PATH</param-name>
    <param-value>/home/jboss/jboss/certstore/cudvm.store</param-value>
  </context-param>
  <context-param>
    <param-name>ARHIVE_AUDIT_FUNC</param-name>
    <param-value>/home/jboss/jboss/data/audit/func/</param-value>
  </context-param>
   <context-param>
    <param-name>ARHIVE_AUDIT_SYS</param-name>
    <param-value>/home/jboss/jboss/data/audit/sys/</param-value>
  </context-param>
  <context-param>
    <param-name>ARHIVE_TOKEN</param-name>
    <param-value>/home/jboss/jboss/data/audit/token/</param-value>
  </context-param>
  <context-param>
    <param-name>UCCERT</param-name>
    <param-value>/home/jboss/jboss/data/uccert/</param-value>
  </context-param>
  <context-param>
    <param-name>AUDIT_SERVICE</param-name>
    <param-value>https://acc.lan.iac.spb.ru:18443/CudServicesPro/AuditService?wsdl</param-value>
  </context-param>
  <context-param>
    <param-name>STS_OBO_SERVICE</param-name>
    <param-value>https://acc.lan.iac.spb.ru:18443/CudServicesSTS/CUDSTS?wsdl</param-value>
  </context-param>
 <context-param>
    <param-name>STS_SERVICE</param-name>
    <param-value>https://acc.lan.iac.spb.ru:18443/CudServicesSTS/CUDSTS?wsdl</param-value>
  </context-param>
  <context-param>
    <param-name>UC_CERT_REESTR</param-name>
    <param-value>http://ca.iac.spb.ru/crl/qual_dump.txt</param-value>
  </context-param>
  <context-param>
    <param-name>CLASSIF_SERVICE</param-name>
    <param-value>http://192.168.2.173:80/WSclassif/ClassifierZip?wsdl</param-value>
  </context-param>
  <context-param>
    <param-name>CLASSIF_LOAD_PATH</param-name>
    <param-value>/home/jboss/jboss/data/classif/LoadScript/sources/</param-value>
  </context-param>
  <context-param>
    <param-name>CLASSIF_LOAD_TMP</param-name>
    <param-value>/home/jboss/jboss/data/classif/LoadScript/tmp/</param-value>
  </context-param>
  <context-param>
    <param-name>JASPER_SERVER</param-name>
    <param-value>http://192.168.68.7:8080/jasperserver/rest_v2/reports/</param-value>
  </context-param>
   <context-param>
    <param-name>JASPER_LOGIN</param-name>
    <param-value>jasperadmin</param-value>
  </context-param>
   <context-param>
    <param-name>JASPER_PASSWORD</param-name>
    <param-value>jasperadmin</param-value>
  </context-param>
  
  <context-param>
    <param-name>org.jboss.jbossfaces.JSF_CONFIG_NAME</param-name>
    <param-value>Mojarra-1.2</param-value>
  </context-param>
  <context-param>
    <param-name>org.richfaces.SKIN</param-name>
    <param-value>blueSky</param-value>
  </context-param>
  <context-param>
    <param-name>facelets.SKIP_COMMENTS</param-name>
    <param-value>true</param-value>
  </context-param>
  <context-param>
    <param-name>org.richfaces.LoadStyleStrategy</param-name>
    <param-value>ALL</param-value>
  </context-param>
  <context-param>
    <param-name>org.richfaces.LoadScriptStrategy</param-name>
    <param-value>ALL</param-value>
  </context-param>
  <context-param>
    <param-name>org.ajax4jsf.COMPRESS_SCRIPT</param-name>
    <param-value>false</param-value>
  </context-param>
  <context-param>
    <param-name>org.richfaces.CONTROL_SKINNING</param-name>
    <param-value>disable</param-value>
  </context-param>
  <context-param>
    <param-name>org.richfaces.CONTROL_SKINNING_CLASSES</param-name>
    <param-value>disable</param-value>
  </context-param>
  <listener>
    <listener-class>iac.grn.infosweb.session.SessionListener</listener-class>
  </listener>
  <listener>
    <listener-class>org.jboss.seam.servlet.SeamListener</listener-class>
  </listener>
  <filter>
    <filter-name>Seam Filter</filter-name>
    <filter-class>org.jboss.seam.servlet.SeamFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Seam Filter</filter-name>
    <url-pattern>*.seam</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>Seam Resource Servlet</servlet-name>
    <servlet-class>org.jboss.seam.servlet.SeamResourceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Seam Resource Servlet</servlet-name>
    <url-pattern>/seam/resource/*</url-pattern>
  </servlet-mapping>
  <context-param>
    <param-name>facelets.DEVELOPMENT</param-name>
    <param-value>false</param-value>
  </context-param>
  <context-param>
    <param-name>facelets.REFRESH_PERIOD</param-name>
    <param-value>2</param-value>
  </context-param>
  <context-param>
    <param-name>javax.faces.DEFAULT_SUFFIX</param-name>
    <param-value>.xhtml</param-value>
  </context-param>
  <security-constraint>
    <display-name>Restrict raw XHTML Documents</display-name>
    <web-resource-collection>
      <web-resource-name>XHTML</web-resource-name>
      <url-pattern>*.xhtml</url-pattern>
    </web-resource-collection>
    <auth-constraint/>
  </security-constraint>
  <filter>
    <display-name>PortFilter</display-name>
    <filter-name>PortFilter</filter-name>
    <filter-class>iac.grn.infosweb.filter.PortFilter</filter-class>
    <init-param>
      <description></description>
      <param-name>available_ports</param-name>
      <param-value>8080,8443,18443</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>PortFilter</filter-name>
    <url-pattern>*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
</web-app>